    "jest-environment-jsdom": "^29.7.0",
    "vite": "^5.0.10"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "js"
    ],
    "testMatch": [
      "**/tests/**/*.test.js"
